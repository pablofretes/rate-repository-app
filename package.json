    "expo-web-browser": "~8.6.0",
    "formik": "^2.2.9",
    "graphql": "^15.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-paper": "^4.9.2",
    "react-native-web": "^0.17.1",
